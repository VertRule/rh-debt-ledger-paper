{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "R16 PQ Tooling Receipt",
  "type": "object",
  "properties": {
    "concept_tag": {
      "type": "string",
      "const": "RH-R16-PQ-TOOLING-RECEIPT"
    },
    "backend": {
      "type": "string",
      "enum": ["none", "openssl-oqsprovider", "oqs-toolbox"]
    },
    "backend_version": {
      "type": "string"
    },
    "commands": {
      "type": "object",
      "properties": {
        "openssl": {
          "type": "string",
          "enum": ["present", "absent"]
        },
        "oqs-toolbox": {
          "type": "string",
          "enum": ["present", "absent"]
        }
      },
      "required": ["openssl", "oqs-toolbox"]
    },
    "notes": {
      "type": "string"
    }
  },
  "required": ["concept_tag", "backend", "backend_version", "commands"],
  "additionalProperties": false
}
